import java.util.*;
public class BellCurve {
	public static void main(String [] args){
		Random rnd= new Random(System.currentTimeMillis());
		for(int i= 0; i < times; i++){
			
		}
		rollDice(10, rnd);
	}
	public static int rollDice(int times, Random rnd){
		int total = 0;
		for(int i = 0; i < times; i++){
			total += rnd.nextInt(5)+1;
		}
		return total;
	}
}
